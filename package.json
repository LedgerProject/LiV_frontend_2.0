{
  "name": "web-client-liv",
  "description": "Project LiV web client",
  "version": "0.1.0-rc.1",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn rsc $npm_package_version"
    }
  },
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "yarn lint:styles && yarn lint:scripts",
    "lint:styles": "stylelint \"src/**/*.{vue,css,scss}\" --syntax scss --max-warnings=0",
    "lint:scripts": "eslint \"{src,config}/**/*.{js,vue}\" --cache --fix --max-warnings=0",
    "rsc": "node scripts/release-sanity-check.js",
    "analyze-bundle": "vue-cli-service build --mode bundle-analyzer"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "husky": "2.4.0",
    "i18next": "^20.3.0",
    "i18next-browser-languagedetector": "^6.1.1",
    "loglevel": "^1.7.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "nprogress": "^0.2.0",
    "resolve-url-loader": "^3.1.2",
    "url-loader": "^4.1.1",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^2.1.0",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^7.27.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "mitt": "^2.1.0",
    "node-sass": "^4.12.0",
    "responsive-loader": "^2.3.0",
    "sass-loader": "^8.0.2",
    "sharp": "^0.28.3",
    "stylelint": "^13.12.0",
    "stylelint-declaration-strict-value": "^1.7.9",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "unused-webpack-plugin": "^2.4.0",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
